    @{
        string fullPath = HttpContext.Current.Request.Url.AbsolutePath;
        string paramsVal = Request.QueryString["forecastType"];

        string url = System.IO.Path.GetFileName(fullPath);
        if (!string.IsNullOrEmpty(paramsVal) && paramsVal == "edit")
        {
            if (url.ToLower().Contains("actualcostconfirm"))
            {
                if (ViewBag.UserRole == "admin" || ViewBag.UserRole == "editor")
                {
                    @* <div class="page-title-box">
                            <h3>
                                <button type="button" class="btn btn-dark" id="update_forecast_history" style="display:none;line-height:1;">save</button>
                            </h3>
                        </div> *@
                }

            }
        }
        else if (url.ToLower().Contains("actualcostconfirm"))
        {
            if (ViewBag.UserRole == "admin" || ViewBag.UserRole == "editor")
            {
                <div class="page-title-box">
                    <h3>
                        <button type="button" class="btn btn-dark" id="cancel_actual_cost" style="line-height:1;background-color: red;border: none;">取消</button>
                        <button type="button" class="btn btn-dark" id="distribute" style="line-height:1;background-color: #FECB6B;border: none;margin-left: 16px;color:black;">配布する</button>
                        <button type="button" class="btn btn-dark" id="create_actual_cost" style="line-height:1;background-color: green;border: none;margin-left: 16px;">保存</button>
                    </h3>
                </div>
            }
        }
    }

